{
  "private": true,
  "version": "1.1.17",
  "name": "sub-rooms-written",
  "scripts": {
    "start": "npm run start:dev",
    "build": "npm run build:master",
    "======================================== hongtangyun start ========================================": "",
    "start:dev": "cross-env app_platform=hongtangyun PORT=9081 REACT_APP_ENV=dev umi dev",
    "start:qa": "cross-env app_platform=hongtangyun PORT=9081 REACT_APP_ENV=qa umi dev",
    "start:master": "cross-env app_platform=hongtangyun PORT=9081 REACT_APP_ENV=master umi dev",
    "build:master": "cross-env is_build=true app_platform=hongtangyun REACT_APP_ENV=master umi build",
    "build:master:analyze": "cross-env ANALYZE=1 is_build=true app_platform=hongtangyun REACT_APP_ENV=master umi build",
    "build:qa": "cross-env is_build=true app_platform=hongtangyun REACT_APP_ENV=qa umi build",
    "build:dev": "cross-env is_build=true app_platform=hongtangyun REACT_APP_ENV=dev umi build",
    "zip:master": "cross-env app_platform=hongtangyun npm run build:master && node script.js",
    "zip:qa": "cross-env app_platform=hongtangyun npm run build:qa && node script.js",
    "zip:dev": "cross-env app_platform=hongtangyun npm run build:dev && node script.js",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
