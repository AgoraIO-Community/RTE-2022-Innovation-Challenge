<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="@dimen/sw_200dp"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/sw_20sp"
        android:paddingEnd="@dimen/sw_50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/iv_title_start_call"
        android:layout_width="@dimen/sw_35dp"
        android:layout_height="@dimen/sw_35dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_title_group_more"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/icon_start_call"
        android:padding="@dimen/sw_5dp"
        android:visibility="gone"
        />


    <ImageView
        android:id="@+id/iv_title_group_more"
        android:layout_width="@dimen/sw_30dp"
        android:layout_height="@dimen/sw_35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/icon_group_more"
        android:paddingTop="@dimen/sw_5dp"
        android:paddingBottom="@dimen/sw_5dp"
        android:paddingStart="@dimen/sw_5dp"
        android:visibility="gone"
        />

</androidx.constraintlayout.widget.ConstraintLayout>