/* miniprogram/pages/videochathelp/videochatehelp.wxss */
/* pages/meeting/meeting.wxss */
@import "../../style/common.wxss";

page {
  height: 100%;
}

.content {
  margin-top: 70px;
  width: 100%;
  position: absolute;
}

.content .footer {
  width: 100%;
  font-size: 24rpx;
  justify-content: flex-end;
  height: 150px;
  position: fixed;
  bottom: 0;
}

.content .footer .toolbar {
  /* border-top: 1px solid #A5A5A5; */
  width: 100%;
  height: 64px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.content .footer .mic.btn {
  background-size: 50% 50%;
  background-image: url('data:image/png;base64,');
}

.content .footer .mic.muted {
  background-image: url('data:image/png;base64,');
}

.content .footer .btn {
  height: 100%;
  width: 44px;
  background-size: 75% 50%;
  border: 0;
  background-position: center;
  background-repeat: no-repeat;
}


.content .footer .hangup.hover{
  background-image: url('data:image/png;base64,');
}

.content .footer .hangup {
  background-image: url('data:image/png;base64,');
}

.content .footer .camera.hover{
  background-image: url('data:image/png;base64,');
}

.content .footer .camera{
  background-image:url('data:image/png;base64,');
}




.content .video-container {
  display: flex;
  width: 100%;
  overflow: hidden;
  flex-wrap: wrap;
}